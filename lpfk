sqlite3 $FK/packages/packages.db "SELECT name AS '扩展名称',substr(url,instr(url,'/')+2,9) AS '网址',substr(substr(substr(url,instr(url,'/')+2,length(url)),instr(substr(url,instr(url,'/')+2,length(url)),'/')+1,length(substr(url,instr(url,'/')+2,length(url)))),0,instr(substr(substr(url,instr(url,'/')+2,length(url)),instr(substr(url,instr(url,'/')+2,length(url)),'/')+1,length(substr(url,instr(url,'/')+2,length(url)))),'/')) AS '作者',substr(hash,0,7) AS '哈希值',case enabled when 1 then 'ture' else 'false' end AS '启用状态' FROM packages;" --cmd ".headers on" --cmd ".mode markdown"